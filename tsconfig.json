{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "build",
    "noImplicitAny": true,
    "removeComments": true,
    "strict": true,
    // "noUnusedParameters": true,
    "skipLibCheck": true,
    "noEmitOnError": true
  },
  "include": ["./src"]
}
